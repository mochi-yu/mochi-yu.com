import Header from '@components/ui/header.astro';
import Footer from '@components/ui/footer.astro';

interface Props {
  title?: string;
}

const { title } = Astro.props;
---

<HtmlBaseLayout title={title}>
  <Header />
  <slot />
  <Footer />
</HtmlBaseLayout>
