---
interface Props {
  title: string
}

const { title } = Astro.props
---

<!doctype html>
<html lang='ja'>
  <head>
    <meta name='description' content='もっちーのポートフォリオサイトです。' />
    
    <link rel='icon' type='image/svg+xml' href='/favicon.png' />
    <meta name='generator' content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <slot />
  </body>
</html>
